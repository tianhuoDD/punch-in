{
	"name": "punch-in",
	"version": "0.3.3",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite --open",
		"build": "vite build",
		"preview": "vite preview",
		"lint:eslint": "eslint . --fix",
		"lint:prettier": "prettier --write .",
		"prepare": "husky",
		"lint:lint-staged": "lint-staged",
		"commit": "git-cz"
	},
	"lint-staged": {
		"*.{js,ts,vue}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{cjs,json}": [
			"prettier --write"
		],
		"*.{vue,html}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{scss,css}": [
			"prettier --write"
		],
		"*.md": [
			"prettier --write"
		]
	},
	"dependencies": {
		"@capacitor/core": "^6.2.0",
		"@icon-park/vue-next": "^1.4.2",
		"@rom/axios": "workspace:^",
		"axios": "^1.7.7",
		"pinia": "^2.1.7",
		"vant": "^4.9.6",
		"vue": "^3.4.29",
		"vue-router": "^4.3.3"
	},
	"devDependencies": {
		"@capacitor/android": "^6.2.0",
		"@capacitor/cli": "^6.2.0",
		"@vant/auto-import-resolver": "^1.2.1",
		"@vitejs/plugin-vue": "^5.0.5",
		"@vueuse/core": "^12.8.2",
		"commitizen": "^4.3.1",
		"commitlint": "^19.6.1",
		"cz-git": "^1.11.0",
		"husky": "^9.1.7",
		"lint-staged": "^15.2.10",
		"unplugin-auto-import": "^0.18.3",
		"unplugin-vue-components": "^0.27.4",
		"vite": "^5.3.1",
		"vite-plugin-svg-icons": "^2.0.1"
	}
